# Database Configuration
# For local PostgreSQL:
# DATABASE_URL=postgresql+psycopg://postgres:password@localhost:5432/health_management
# For Supabase (connection pooler - recommended for application):
DATABASE_URL=postgresql+psycopg://[user]:[password]@[host]:6543/[database]?sslmode=require
# For Supabase migrations (direct connection):
# DATABASE_URL_MIGRATION=postgresql+psycopg://[user]:[password]@[host]:5432/[database]?sslmode=require

DB_BACKEND=postgresql

# Environment
ENVIRONMENT=development

# Server Configuration
PORT=8000

# CORS Origins (comma-separated)
CORS_ORIGINS=http://localhost:5173,http://localhost:3000
